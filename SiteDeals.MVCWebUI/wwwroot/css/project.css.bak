/*uteksan*/
@import url('https://fonts.googleapis.com/css2?family=DM+Sans:ital,wght@0,400;0,500;0,700;1,400;1,500;1,700&display=swap');

@font-face {
    font-family: 'Tuffy';
    src: url('../fonts/Tuffy.woff2') format('woff2'), url('../fonts/Tuffy.woff') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote {
    margin: 0;
    padding: 0;
}

a {
    outline: 0;
    cursor: pointer;
    text-decoration: none !important;
}

body {
    font-family: 'DM Sans', sans-serif;
    font-weight: 400;
    font-size: 16px;
    color: #000000;
    background: #E5E5E5;
}

header {
    background: #fff;
}

.navScrolled {
    position: fixed;
    top: 0;
    z-index: 99;
    box-shadow: 0px 1px 5px #000;
    background: #fff;
    width: 100%;
}

.color-red {
    color: #EB5757;
}

.color-green {
    color: #219653;
}

.dipnot {
    color: #B5B8CB;
    font-size: 11px;
    padding: 15px 0;
}

.moreAbsolute {
    position: absolute;
}

#containerHeader {
    box-shadow: inset 0px -0.5px 0px rgb(30 38 97 / 80%);
    padding: 18px 0;
}

    #containerHeader .logo a {
        font-family: 'Tuffy';
        font-style: normal;
        font-weight: 700;
        font-size: 32px;
        line-height: 47px;
    }

    #containerHeader .twoList {
        display: flex;
        justify-content: space-between;
        align-items: center;
    }

    #containerHeader .headerLogin {
        display: flex;
    }

        #containerHeader .headerLogin .item {
            position: relative;
            display: flex;
            align-items: center;
            margin: 0 5px;
        }

            #containerHeader .headerLogin .item:last-of-type {
                margin: 0;
            }

        #containerHeader .headerLogin .searchIcon {
            position: absolute;
            cursor: pointer;
            left: 7px;
            top: 5px;
        }

        #containerHeader .headerLogin .search-bar {
            padding: 0 5px 0 35px;
            border: 0.5px solid #1E2661;
            box-sizing: border-box;
            border-radius: 4px;
            height: 36px;
        }

        #containerHeader .headerLogin .membership-login-link {
            border: 1px solid #644BD7;
            box-sizing: border-box;
            border-radius: 4px;
            height: 36px;
            display: flex;
            align-items: center;
            color: #644BD7;
            padding: 0 15px;
            font-size: 14px;
        }

        #containerHeader .headerLogin .membership-register-link {
            background: #644BD7;
            border-radius: 4px;
            height: 36px;
            display: flex;
            align-items: center;
            color: #FFFFFF;
            line-height: 14px;
            letter-spacing: 0.01em;
            padding: 0 15px;
            font-size: 14px;
        }

        #containerHeader .headerLogin .messages-login {
            width: 40px;
            background: #F2F2F2;
            display: flex;
            align-items: center;
            justify-content: center;
            height: 40px;
            border-radius: 20px;
            color: #644BD7;
        }

        #containerHeader .headerLogin .notifications-login {
            width: 40px;
            background: #F2F2F2;
            display: flex;
            align-items: center;
            justify-content: center;
            height: 40px;
            border-radius: 20px;
            color: #644BD7;
        }

        #containerHeader .headerLogin .add-product-header {
            background: #644BD7;
            border-radius: 4px;
            border: 0;
            height: 40px;
            padding: 0 30px;
            color: #fff;
            font-weight: 700;
            margin-left: 5px;
        }

#breadCrumb {
    padding: 22px 0;
    box-shadow: inset 0px -0.5px 0px rgba(30, 38, 97, 0.8);
}

    #breadCrumb .breadCrumb_UT a {
        position: relative;
        line-height: 21px;
        letter-spacing: 0.01em;
        color: #1E2661;
    }

        #breadCrumb .breadCrumb_UT a:after {
            font-family: "Material Icons";
            content: "\e5cc";
            padding: 0 10px;
            position: relative;
            top: 2px;
        }

        #breadCrumb .breadCrumb_UT a:last-of-type:after {
            display: none;
        }

.productsDetail {
    background: #fff;
    margin: 8px 0;
}

    .productsDetail p {
        letter-spacing: 0.01em;
        color: #333333;
        padding: 10px 0;
    }

    .productsDetail .twoList {
        display: flex;
        flex-wrap: wrap;
        padding: 20px 0;
    }

        .productsDetail .twoList > .item {
            width: calc(50% - 10px);
            margin-right: 20px;
        }

            .productsDetail .twoList > .item:last-of-type {
                margin-right: 0;
            }

        .productsDetail .twoList .detailSlider {
            padding: 0 15px;
        }

        .productsDetail .twoList .productsText {
            padding: 0 15px;
        }

.productsText .productsTop {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}

    .productsText .productsTop > span {
        border: 1px solid #B5B8CB;
        box-sizing: border-box;
        border-radius: 8px;
        padding: 5px 20px;
        display: flex;
    }

        .productsText .productsTop > span > i {
            display: inline-flex;
            flex-wrap: wrap;
            align-items: center;
            height: 48px;
            padding: 0 5px;
            font-style: normal;
            font-size: 20px;
            color: #644BD7;
            font-weight: 700;
        }

            .productsText .productsTop > span > i > span {
                font-size: 30px;
            }

.productsText .product-information h1 {
    font-weight: 700;
    font-size: 20px;
    line-height: 26px;
    letter-spacing: 0.01em;
    color: #1E2661;
    padding: 20px 0;
}

.productsText .product-information .price-new {
    font-size: 34px;
    line-height: 44px;
    letter-spacing: 0.01em;
    color: #644BD7;
}

.productsText .product-price-container {
    display: flex;
    flex-wrap: wrap;
    padding: 5px 0;
}

    .productsText .product-price-container > span {
        margin: 0 15px 0 0;
        color: #828282;
    }

    .productsText .product-price-container .originalPrice {
        font-weight: 500;
        font-size: 20px;
        line-height: 26px;
        letter-spacing: 0.01em;
        text-decoration-line: line-through;
    }

    .productsText .product-price-container .discount-amount-new {
        font-weight: 500;
        font-size: 20px;
        line-height: 26px;
        letter-spacing: 0.01em;
    }

    .productsText .product-price-container .delivery-options-content-list {
        display: flex;
        flex-wrap: wrap;
    }

        .productsText .product-price-container .delivery-options-content-list > i {
            font-style: normal;
            padding: 0 5px 0;
        }

        .productsText .product-price-container .delivery-options-content-list .free-shipping {
            letter-spacing: 0.01em;
            color: #4F4F4F;
        }

    .productsText .product-price-container .delivery-options-content-company {
        border-left: 1px solid #828282;
        margin: 0 5px;
        letter-spacing: 0.01em;
        color: #F2994A;
        font-weight: 700;
    }

.productsText .addToCartButton {
    margin-top: 50px;
}

    .productsText .addToCartButton > button {
        width: 100%;
        background: #644BD7;
        border-radius: 8px;
        height: 58px;
        display: flex;
        align-items: center;
        justify-content: center;
        border: none;
        cursor: pointer;
        position: relative;
    }

        .productsText .addToCartButton > button > span {
            display: flex;
            align-items: center;
        }

    .productsText .addToCartButton .go-to-product {
        font-weight: 700;
        font-size: 20px;
        letter-spacing: 0.01em;
        color: #FFFFFF;
    }

    .productsText .addToCartButton .img {
        right: 30px;
        position: absolute;
    }

        .productsText .addToCartButton .img img {
            width: 24px;
            height: 24px;
            filter: brightness(0) invert(1);
        }

.pagination-content-comment-container {
    background: #fff;
    padding: 0 0 25px;
}

.product-seller-container {
    display: flex;
    flex-wrap: wrap;
    align-items: flex-end;
    justify-content: space-between;
    margin-top: 30px;
}

    .product-seller-container > span {
