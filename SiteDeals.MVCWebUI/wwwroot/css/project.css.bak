        display: flex;
        align-items: center;
    }

        .product-seller-container > span:last-of-type {
            margin-right: 0;
        }

    .product-seller-container .username {
        display: flex;
        flex-direction: column;
    }

        .product-seller-container .username .adding {
            letter-spacing: 0.01em;
            color: #333333;
            text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
            display: block;
            margin-bottom: 10px;
        }

    .product-seller-container i {
        font-style: normal;
    }

    .product-seller-container .product-ratings-container {
        display: flex;
        flex-wrap: wrap;
    }

        .product-seller-container .product-ratings-container .item {
            width: 33% !important;
            display: flex;
            padding-right: 10px;
            align-items: center;
            color: #333333;
        }

            .product-seller-container .product-ratings-container .item i > img {
                width: 24px;
                height: 24px;
            }

            .product-seller-container .product-ratings-container .item > i {
                display: flex;
                flex-wrap: wrap;
                align-items: center;
                padding: 0 5px 0;
            }

    .product-seller-container .product-seller-container > span {
        display: flex;
        align-items: center;
        flex-wrap: wrap;
        margin-right: 20px;
    }

        .product-seller-container .product-seller-container > span:last-of-type {
            margin-right: 0;
        }

    .product-seller-container .product-ratings-container .item:last-of-type {
        padding-right: 0;
    }

.product-date-of-insertion {
    width: 100%;
    margin: 10px 0;
    display: inline-block;
}

    .product-date-of-insertion .date-of-insertion-container {
        border: 1px solid #BDBDBD;
        box-sizing: border-box;
        border-radius: 4px;
        margin: 0 15px;
        display: flex;
        padding: 10px 15px;
        color: #828282;
    }

        .product-date-of-insertion .date-of-insertion-container > span {
            margin: 0 10px 0 0;
        }

        .product-date-of-insertion .date-of-insertion-container .date-of-insertion-title {
            letter-spacing: 0.01em;
        }

.product-description-area .product-description-container {
    padding: 0 15px;
}

.social-media-share {
    display: inline-flex;
    flex-wrap: wrap;
    margin: 20px 0 30px;
}

    .social-media-share .facebook a {
        background: #3B5998;
        border-radius: 4px;
    }

    .social-media-share .twitter a {
        background: #55ACEE;
        border-radius: 4px;
    }

    .social-media-share a {
        color: #fff;
        width: 255px;
        display: block;
        text-align: center;
        font-weight: 700;
        padding: 13px 0;
        margin-right: 30px;
    }

.pagination-content-comment-area .pagination-content-comment-container .pagination-content-comment-title {
    background: #644BD7;
    border-radius: 4px;
    padding: 0 15px;
    display: flex;
    justify-content: space-between;
    height: 50px;
    align-items: center;
    color: #fff;
    font-weight: 700;
}

    .pagination-content-comment-area .pagination-content-comment-container .pagination-content-comment-title a {
        color: #fff;
    }

    .pagination-content-comment-area .pagination-content-comment-container .pagination-content-comment-title i {
        font-style: normal;
    }
#comment-area {
    width:100%;
    padding:10px;
    min-height:120px;
}
.pagination-content-comment-item {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    position: relative;
    padding: 25px 0;
    box-shadow: inset 0px -1px 1px rgba(0, 0, 0, 0.25);
}

    .pagination-content-comment-item > div:nth-child(1) {
        width: 10%;
    }

    .pagination-content-comment-item > div:nth-child(2) {
        width: 85%;
    }

    .pagination-content-comment-item > div:nth-child(3) {
        width: 5%;
    }

    .pagination-content-comment-item .comment-profil {
        text-align: center;
    }

    .pagination-content-comment-item .comment-center p {
        letter-spacing: 0.01em;
        color: #333333;
        padding: 15px 0;
    }

    .pagination-content-comment-item .comment-center .comment-title {
        letter-spacing: 0.01em;
        color: #644BD7;
        font-weight: 700;
    }

    .pagination-content-comment-item .comment-center .comment-time {
        letter-spacing: 0.01em;
        color: #BDBDBD;
        margin-left: 10px;
    }

    .pagination-content-comment-item .comment-right {
        display: flex;
        flex-direction: column;
        align-items: center;
    }

        .pagination-content-comment-item .comment-right .number-likes {
            position: absolute;
            bottom: 25px;
            display: flex;
            color: #644BD7;
            right: 10px;
        }

            .pagination-content-comment-item .comment-right .number-likes span {
                padding: 0 5px;
            }

        .pagination-content-comment-item .comment-right .comment-s1 {
            font-size: 30px;
            color: #644BD7;
            position: absolute;
            top: 0;
        }

    .pagination-content-comment-item .comment-center .twolike {
        display: flex;
        flex-wrap: wrap;
    }

        .pagination-content-comment-item .comment-center .twolike a {
            display: flex;
            margin-right: 20px;
            letter-spacing: 0.01em;
            color: #333333;
            font-weight: bold;
        }

            .pagination-content-comment-item .comment-center .twolike a span {
                margin-right: 10px;
            }

.comment-container-bottom .comment-text {
    padding: 0 15px;
}

    .comment-container-bottom .comment-text .title {
        color: #000000;
        font-weight: 700;
        padding: 10px 0;
    }

    .comment-container-bottom .comment-text .comment-two {
        display: flex;
        flex-wrap: wrap;
