@model List<SiteDeals.MVCWebUI.Areas.Identity.Data.SiteDealsMVCWebUIUser>
@{
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}
<table class=d-lg-table-row>
    <tr>
        <td>UserID</td>
        <td>UserName</td>
        <td>Email</td>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">Hiç bir üye yoktur</td>
        </tr>
    }
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td>@item.Email</td>
           
        </tr>
    }
</table>